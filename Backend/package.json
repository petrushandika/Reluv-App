{
  "name": "reluv-app",
  "version": "0.0.1",
  "description": "<p align=\"center\">\r   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"120\" alt=\"Nest Logo\" /></a>\r </p>",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "postinstall": "npx prisma migrate deploy && npx prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "dotenv -e .env -- npx prisma generate",
    "prisma:studio": "dotenv -e .env -- npx prisma studio",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev",
    "prisma:migrate:deploy": "dotenv -e .env -- npx prisma migrate deploy",
    "prisma:migrate:status": "dotenv -e .env -- npx prisma migrate status",
    "prisma:reset": "dotenv -e .env -- npx prisma migrate reset --force",
    "prisma:push": "dotenv -e .env -- npx prisma db push",
    "db:generate": "npm run prisma:generate",
    "db:migrate": "npm run prisma:migrate",
    "db:migrate:deploy": "npm run prisma:migrate:deploy",
    "db:migrate:status": "npm run prisma:migrate:status",
    "db:reset": "npm run prisma:reset",
    "db:push": "npm run prisma:push",
    "db:seed": "dotenv -e .env -- ts-node prisma/seed.ts",
    "db:reset:seed": "npm run db:reset && npm run db:seed",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:refresh": "npm run db:reset:seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@types/pg": "^8.15.6",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.8.0",
    "compression": "^1.7.4",
    "dotenv": "^16.6.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.3",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.8.0",
    "rxjs": "^7.8.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.11",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.15.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.19.1",
    "@types/nodemailer": "^6.4.21",
    "@types/passport-facebook": "^3.0.4",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^11.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^7.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "keywords": [],
  "type": "commonjs"
}
